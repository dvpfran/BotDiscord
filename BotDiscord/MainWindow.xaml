<Window x:Class="BotDiscord.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BotDiscord"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen" ResizeMode="NoResize"
        Title="Fala Sozinhoooooo" Height="450" Width="368" Loaded="Window_Loaded">
    <Grid Name="grd" Margin="5">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="220"/>
            <ColumnDefinition Width="70"/>
            <ColumnDefinition Width="50"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="25"/>
        </Grid.RowDefinitions>

        <!--<Button Grid.Column="0" Grid.Row="0" Name="btnTeste" Height="20" Click="btnTeste_Click" Margin="201,3,0,3">+</Button>-->
        <Button Grid.Column="2" Grid.Row="0" Name="btnAdd" HorizontalAlignment="Right" Width="20" Height="20" Click="btnAdd_Click">+</Button>
    </Grid>
</Window>
